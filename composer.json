{
    "name": "itspire/common",
    "description": "Itspire Common",
    "type": "library",
    "keywords": ["itspire"],
    "homepage": "http://www.itspire.fr",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ryan RAJKOMAR",
            "email": "ryan.rajkomar@itspire.fr"
        }
    ],
    "require": {
        "php": "~8.1",
        "symfony/expression-language": "~5.4",
        "symfony/validator": "~5.4",
        "doctrine/collections": "~1.6",
        "jms/serializer": "~3.17",
        "psr/http-message": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Itspire\\Common\\": "src/main/php"
        }
    },
    "require-dev": {
        "nyholm/psr7": "~1.5",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "~3.x-dev",
        "symfony/phpunit-bridge": "~6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Itspire\\Common\\Tests\\": "src/test/php"
        }
    }
}
